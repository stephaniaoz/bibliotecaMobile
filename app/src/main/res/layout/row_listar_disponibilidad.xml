<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_margin="5dp"
              android:elevation="10dp"
              android:padding="4dp"
              android:paddingBottom="10dp"
              android:foreground="?selectableItemBackground">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:elevation="20dp"
            android:background="@drawable/border_listview_row">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="codBarras"
                    style="@style/encabezadoItem"/>

                <TextView
                    android:id="@+id/tvCodBarras"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:layout_below="@+id/textView"/>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Localización"
                    style="@style/encabezadoItem"
                    android:layout_below="@+id/tvCodBarras"/>

                <TextView
                    android:id="@+id/tvLocalizacion"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:layout_below="@+id/textView3"/>

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Estante"
                    style="@style/encabezadoItem"
                    android:layout_below="@+id/tvLocalizacion"/>

                <TextView
                    android:id="@+id/tvEstante"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:layout_below="@+id/textView5"/>

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Signatura"
                    style="@style/encabezadoItem"
                    android:layout_below="@+id/tvEstante"/>

                <TextView
                    android:id="@+id/tvSignatura"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:layout_below="@+id/textView7"/>

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Estado"
                    style="@style/encabezadoItem"
                    android:layout_below="@+id/tvSignatura"/>

                <TextView
                    android:id="@+id/tvEstado"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:layout_below="@+id/textView9"/>

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Categoría"
                    style="@style/encabezadoItem"
                    android:layout_below="@+id/tvEstado"/>

                <TextView
                    android:id="@+id/tvCategoria"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    android:layout_below="@+id/textView11"/>

        </RelativeLayout>


</LinearLayout>